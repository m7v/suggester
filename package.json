    "ua-parser-js": "^0.7.17"
  },
  "devDependencies": {
    "bundlesize": "^0.16.0",
    "babel-eslint": "^7.2.3",
    "chai": "^3.5.0",
    "codacy-coverage": "^2.0.2",
    "coveralls": "^2.13.1",
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-react": "^7.0.1",
    "fetch-mock": "^5.13.1",
    "gh-pages": "^0.12.0",
    "jsdom": "8.0.4",
    "mocha": "2.4.5",
    "mocha-lcov-reporter": "^1.3.0",
    "nock": "^9.0.13",
    "pre-push": "^0.1.1",
    "react-test-renderer": "^15.6.1",
    "redux-devtools-extension": "^2.13.2",
    "redux-mock-store": "^1.3.0",
    "serve": "^5.1.4",
    "stylelint": "^7.10.1",
    "stylelint-config-standard": "^16.0.0"
  },
  "bundlesize": [
    {
      "path": "./build/static/js/main.*.js",
      "maxSize": "125 kB"
    },
    {
      "path": "./build/static/js/*.chunk.js",
      "maxSize": "55 kB"
    }
  ],
  "scripts": {
    "lint": "eslint ./src",
    "flow": "flow",
    "bundlesize": "bundlesize",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "analyze:main": "source-map-explorer ./build/static/js/main.*.js",
    "analyze:chunk": "source-map-explorer ./build/static/js/$CHUNK.*.chunk.js",
    "analyze:all": "yarn analyze:main; yarn analyze:chunk",
    "test": "NODE_ENV=test CI=true react-scripts test --env=jsdom; bundlesize",
    "test:ci": "CI=true react-scripts test --env=jsdom",
    "check:all": "yarn lint && yarn test:ci",
    "eject": "react-scripts eject",
    "serve": "serve -s build",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build"
  }
}
