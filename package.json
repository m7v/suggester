    "react-chartjs": "^0.8.0",
    "react-dom": "^15.5.4",
    "react-helmet": "^5.2.0",
    "react-image": "^1.0.1",
    "react-lazyload": "^2.2.7",
    "react-redux": "^5.0.4",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-scripts": "1.0.17",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.7.2",
    "redux-batched-actions": "^0.2.0",
    "redux-logger": "^3.0.1",
    "redux-orm": "^0.9.4",
